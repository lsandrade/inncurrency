<p id="notice"><%= notice %></p>

<h1>Listing Conversors</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @conversors.each do |conversor| %>
      <tr>
        <td><%= link_to 'Show', conversor %></td>
        <td><%= link_to 'Edit', edit_conversor_path(conversor) %></td>
        <td><%= link_to 'Destroy', conversor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Conversor', new_conversor_path %>
